    <script>
      let countUpCont = document.querySelector("#cont5");
      let view = Math.round(window.innerHeight) / 2;
      let active = false;
      console.log(view);
      window.addEventListener("scroll", () => {
        console.log(window.scrollY);
        console.log(countUpCont.offsetTop);
        if (window.scrollY >= countUpCont.offsetTop - view) {
          if (active == false) {
            var counter1 = new CountUp("count1", 0, 989, 0, 8, {});
            counter1.start();
            var counter2 = new CountUp("count2", 0, 23907, 0, 5);
            counter2.start();
            var counter3 = new CountUp("count3", 0, 34, 0, 10);
            counter3.start();
            var counter4 = new CountUp("count4", 0, 176, 0, 10);
            counter4.start();
            active = true;
          }
        }
      });
    </script>
